procedure main()
write(readline("input.txt",1)|"failed")
write(readline("input.txt",2)|"failed")

end
 
procedure readline(f,n)                         # return n'th line of file f
f := open(\f,"r") | fail                        # open file
every  i := n & line := |read(f) \ n do i -:= 1  #  <== here
close(f)
if i = 0 then return line
end